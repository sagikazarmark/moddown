[please]
version = 15.0.0

[go]
importpath = github.com/sagikazarmark/moddown

[buildconfig]
golangci-lint-version = 1.30.0
hub-tool = "///pleasings2//tools/github:hub"
hub-version = 2.14.2

[alias "lint"]
desc = Runs linters for this repo
cmd = run ///pleasings2//tools/go:golangci-lint -- run

[alias "publish"]
desc = Publish build artifacts to GitHub
cmd = run :publish --
